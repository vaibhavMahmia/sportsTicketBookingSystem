<div class="sectionLight">
  <div class="row">
    <div class="col-sm-12">
      <div class="jumbotron jumbotron-fluid">
        <div class="container">
          <h1 class="display-4">Sports Ticket Booking</h1>
          <p class="lead">{{uname}}</p>
          
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col"></div>
    <div class="col-sm-12 col-md-6">
      <div id="mainCarousel" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
          <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
          <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
          <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
          <div class="carousel-item active">
            <img class="d-block w-100" src="/icon/first.jpg" alt="First slide" />
            <div class="carousel-caption d-none d-md-block">
              <h3>AEW Dynamite !</h3>
              <p>January 27</p>
            </div>
          </div>
          <div class="carousel-item">
            <img class="d-block w-100" src="/icon/second.png" alt="Second slide" />
          </div>
          <div class="carousel-item">
            <img class="d-block w-100" src="/icon/third.jpg" alt="Third slide" />
          </div>
        </div>
        <a class="carousel-control-prev" href="#mainCarousel" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#mainCarousel" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
      <!-- <br><br><br> -->
    </div>
    <div class="col"></div>
  </div>
</div>
